<Page x:Class="cibyt_pri.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:cibyt_pri"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="HomePage">

    <Grid Background="{StaticResource home_background}">
        <StackPanel VerticalAlignment="Top">
            <Label Content="Home" Background="{StaticResource home_color}" FontFamily="Segoe UI Semibold" FontSize="18" FontWeight="Bold" />
            
        </StackPanel>
            
    </Grid>
</Page>
